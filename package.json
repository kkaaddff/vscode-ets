{
  "name": "arkts-ets",
  "displayName": "ArkTs",
  "description": "ArkTs ets 语法高亮插件",
  "publisher": "kkaaddff",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "ets",
        "aliases": [
          "ArkTs",
          "ets"
        ],
        "extensions": [
          ".ets"
        ],
        "configuration": "./syntaxes/ets-language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "ets",
        "scopeName": "source.ets",
        "path": "./syntaxes/ets.tmLanguage.json"
      }
    ]
  }
}
